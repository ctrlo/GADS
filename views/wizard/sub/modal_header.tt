<div class="modal-header d-flex justify-content-between align-items-baseline">
  <div class="modal-header__content">
    <h3 class="modal-title" id="[% modalId %]Label">
      [% label | html %]
    </h3>
  </div>
  <button type="button" class="btn close m-0 p-0" data-bs-dismiss="modal" aria-label="Close">
    <span aria-hidden="true" class="hidden">Close</span>
  </button>
</div>
