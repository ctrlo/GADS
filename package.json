{
  "name": "Linkspace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rollup": "rollup --config rollup.config.js",
    "sass": "sass --update --no-source-map sass:public/css/",
    "jshint": "jshint public/js/*.js",
    "ts": "yarn run webpack --watch",
    "lint": "eslint src",
    "start": "npm-watch"
  },
  "dependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "awesome-typescript-loader": "^5.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "form-serialize": "^0.7.2",
    "handlebars": "^4.7.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.18.3",
    "react-modal": "^3.11.2",
    "sass": "^1.26.5",
    "sass-loader": "^10.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-plugin-jsdoc": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jshint": "^2.11.0",
    "npm-watch": "^0.6.0",
    "prettier-eslint": "^9.0.1",
    "rollup": "^2.8.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "watch": {
    "rollup": {
      "patterns": "src",
      "extensions": "js"
    },
    "sass": {
      "patterns": "sass",
      "extensions": "scss"
    },
    "ts": {
      "patterns": "src/dashboard",
      "extensions": "ts,tsx"
    }
  }
}
